<h1>Song Form</h1>
<%= form_for(@song) do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.label :genre_id %>
	<%= f.text_field :genre_id %>
	<%= f.label :artist_id %>
	<%= f.text_field :artist_id %>
	<!-- # <%= f.collection_select :genre_id, Genre.all, :id, :name %>  -->
	<!-- # <%= f.collection_select :artist_id, Artist.all, :id, :name %> -->
	<%= f.submit %>
<% end %>